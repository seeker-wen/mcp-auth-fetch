{
  "name": "mcp-auth-fetch",
  "version": "1.2.0",
  "description": "A library for authenticated fetch requests to MCP services.",
  "scripts": {
    "test": "vitest run",
    "clean": "node -e \"require('fs').rmSync('./dist', { recursive: true, force: true })\"",
    "build": "npm run clean && tsc --module commonjs --outDir dist/cjs && tsc --module esnext --outDir dist/esm",
    "prepare:publish": "npm run build && node prepare-publish.js",
    "publish:pkg": "cd dist && npm publish"
  },
  "keywords": [
    "mcp",
    "auth",
    "fetch"
  ],
  "author": "seeker-wen",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/seeker-wen/mcp-auth-fetch.git"
  },
  "homepage": "https://github.com/seeker-wen/mcp-auth-fetch",
  "dependencies": {
    "fastmcp": "^3.17.0",
    "micromatch": "^4.0.7",
    "node-fetch": "^3.3.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/micromatch": "^4.0.7",
    "@types/node": "^20.14.2",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  }
}